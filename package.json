{
  "name": "interactive-longform-articles",
  "version": "1.0.0",
  "description": "Interactive, multimedia articles for longform journalism",
  "private": true,
  "watch": {
    "all": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx",
      "ignore": "",
      "quiet": true,
      "legacyWatch": true,
      "delay": 500,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "start": "npm-run-all -s all watch",
    "all": "npm-run-all -s run-babel run-webpack",
    "run-babel": "npx babel src --out-dir js",
    "run-webpack": "npx webpack",
    "watch": "npm-watch all"
  },
  "engines": {
    "npm": ">=2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clickonmedia/interactive-longform-articles.git"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "interactive",
    "articles"
  ],
  "author": "CLICKON Media",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/clickonmedia/interactive-longform-articles/issues"
  },
  "homepage": "https://github.com/clickonmedia/interactive-longform-articles",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "browser-sync": "^2.23.6",
    "del": "^3.0.0",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.2",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sequence": "^0.4.6",
    "gulp-watch": "^4.3.11",
    "lodash": "^4.17.15",
    "npm-watch": "^0.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "run-sequence": "^2.2.1",
    "styled-components": "^5.0.0",
    "underscore": "^1.9.2"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "@babel/cli": "^7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
